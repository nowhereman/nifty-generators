<%% semantic_form_for @<%= singular_name %><%- if options[:jquery] -%>, :html => { :'data-remote' => true, :'data-method' => data_method ||= '' } <%- end -%>do |form| %>
  <%% form.inputs do %>
<%- for attribute in attributes -%>
    <%%= form.input :<%= attribute.name %> %>

<%- end -%>
  <%% end %>
  <%% form.buttons do %>
    <%%= form.commit_button %>
  <%% end %>
<%% end %>
